{
  "id": null,
  "title": "Feature Store (Redis) â€” Batch 2.4",
  "uid": "feature-store",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 2,
  "refresh": "10s",
  "tags": ["features", "redis", "ingestion"],
  "templating": {
    "list": [
      {
        "type": "query",
        "name": "domain",
        "label": "Domain",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(feature_writes_total, domain)",
        "includeAll": true,
        "multi": true
      },
      {
        "type": "query",
        "name": "op",
        "label": "Operation",
        "datasource": "Prometheus",
        "refresh": 1,
        "query": "label_values(feature_op_latency_seconds_bucket, op)",
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "API Up",
      "id": 0,
      "datasource": "Prometheus",
      "targets": [
        { "expr": "up{job=\"ingestion-api\"}", "legendFormat": "ingestion-api" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "red", "value": 0 }, { "color": "green", "value": 1 }]
          }
        }
      },
      "gridPos": { "h": 5, "w": 4, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Writes/sec",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(rate(feature_writes_total{domain=~\"$domain\"}[5m]))", "legendFormat": "writes/sec" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Reads/sec",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(rate(feature_reads_total{domain=~\"$domain\"}[5m]))", "legendFormat": "reads/sec" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Hit Ratio (5m)",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(feature_hits_total{domain=~\"$domain\"}[5m])) / (sum(rate(feature_hits_total{domain=~\"$domain\"}[5m])) + sum(rate(feature_misses_total{domain=~\"$domain\"}[5m])))",
          "legendFormat": "hit_ratio"
        }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": {
        "defaults": { "unit": "percentunit", "min": 0, "max": 1 }
      }
    },
    {
      "type": "stat",
      "title": "Misses/sec",
      "id": 4,
      "datasource": "Prometheus",
      "targets": [
        { "expr": "sum(rate(feature_misses_total{domain=~\"$domain\"}[5m]))", "legendFormat": "misses/sec" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Latency p50 / p90 / p99 by op",
      "id": 5,
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "s" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(feature_op_latency_seconds_bucket{op=~\"$op\"}[5m])) by (le, op))",
          "legendFormat": "p50 {{op}}"
        },
        {
          "expr": "histogram_quantile(0.9, sum(rate(feature_op_latency_seconds_bucket{op=~\"$op\"}[5m])) by (le, op))",
          "legendFormat": "p90 {{op}}"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(feature_op_latency_seconds_bucket{op=~\"$op\"}[5m])) by (le, op))",
          "legendFormat": "p99 {{op}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Redis Memory Used",
      "id": 6,
      "datasource": "Prometheus",
      "targets": [
        { "expr": "redis_memory_used_bytes", "legendFormat": "used" }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },
    {
      "type": "stat",
      "title": "Redis Uptime (days)",
      "id": 7,
      "datasource": "Prometheus",
      "targets": [
        { "expr": "redis_uptime_in_seconds / 86400", "legendFormat": "uptime_days" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "timeseries",
      "title": "Writes (1h increase) by domain",
      "id": 8,
      "datasource": "Prometheus",
      "targets": [
        { "expr": "increase(feature_writes_total{domain=~\"$domain\"}[1h])", "legendFormat": "{{domain}}" }
      ],
      "fieldConfig": { "defaults": { "unit": "short" } }
    }
  ]
}